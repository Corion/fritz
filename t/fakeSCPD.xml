    </action>
  </actionList>
  <serviceStateTable>
    <stateVariable sendEvents="yes">
      <name>Argument</name>
      <dataType>string</dataType>
      <defaultValue>0815</defaultValue>
    </stateVariable>
  </serviceStateTable>
</scpd>
